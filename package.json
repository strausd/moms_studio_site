    "babel-preset-react": "^6.24.1",
    "dotenv": "^4.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-jest": "^22.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.0.5",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.4.1",
    "webpack-dev-server": "^2.10.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
